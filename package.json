{
  "name": "socialscalebooster",
  "version": "0.1.0",
  "description": "Smartflow Store MVP API with Multi-Agent Orchestration",
  "type": "module",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "tsx server/index.ts",
    "dev": "tsx watch server/index.ts",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "migrate": "drizzle-kit generate && drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx server/db/seed.ts",
    "pull": "git pull --no-edit",
    "push": "git push",
    "auto-commit": "git add . && git commit -m 'chore: auto-commit [Auto-Sync]' || true",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.1",
    "@tanstack/react-query": "^5.90.18",
    "@types/cors": "^2.8.19",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.5",
    "axios": "^1.13.1",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.562.0",
    "pg": "^8.17.1",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "stripe": "^16.12.0",
    "tailwind-merge": "^3.4.0",
    "wouter": "^3.9.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@replit/vite-plugin-cartographer": "^0.4.4",
    "@replit/vite-plugin-runtime-error-modal": "^0.0.4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "bcryptjs": "^3.0.3",
    "drizzle-kit": "^0.31.8",
    "jsonwebtoken": "^9.0.3",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.15",
    "zod": "^4.3.5"
  },
  "engines": {
    "node": ">=18"
  }
}
